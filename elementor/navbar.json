                                {
                                    "id": "nav-menu-col",
                                    "elType": "column",
                                    "settings": {
                                        "_column_size": 50,
                                        "_inline_size": 80,
                                        "content_position": "center",
                                        "align": "flex-end"
                                    },
                                    "elements": [
                                        {
                                            "id": "nav-menu123",
                                            "elType": "widget",
                                            "settings": {
                                                "editor": "<div class=\"clipup-desktop-menu\" style=\"display: flex; align-items: center; justify-content: flex-end; height: 64px;\">\n  <div style=\"display: flex; align-items: center;\">\n    <a href=\"#como-funciona\" style=\"color: #666; font-size: 14px; text-decoration: none; margin-right: 24px; transition: color 0.3s ease;\">Como Funciona</a>\n    <a href=\"#servicos\" style=\"color: #666; font-size: 14px; text-decoration: none; margin-right: 24px; transition: color 0.3s ease;\">Serviços</a>\n    <a href=\"#earnings\" style=\"color: #666; font-size: 14px; text-decoration: none; margin-right: 24px; transition: color 0.3s ease;\">Ganhos</a>\n    <a href=\"#perguntas\" style=\"color: #666; font-size: 14px; text-decoration: none; margin-right: 24px; transition: color 0.3s ease;\">FAQ</a>\n  </div>\n  \n  <div style=\"display: flex; align-items: center; gap: 12px;\">\n    <a href=\"#\" style=\"display: inline-block; border: none; background-color: var(--e-global-color-primary); color: white; font-size: 14px; font-weight: 500; padding: 8px 16px; border-radius: 6px; text-decoration: none; transition: background-color 0.3s ease;\">Entrar</a>\n    \n    <a href=\"#\" style=\"display: inline-block; border: 1px solid #ddd; background-color: transparent; color: #333; font-size: 14px; font-weight: 500; padding: 8px 16px; border-radius: 6px; text-decoration: none; transition: all 0.3s ease;\">Criar Conta</a>\n  </div>\n</div>\n\n<!-- Mobile Menu Toggle Button -->\n<div class=\"clipup-mobile-menu-toggle\" style=\"display: none; align-items: center; height: 64px;\">\n  <button id=\"clipup-mobile-toggle\" style=\"background: none; border: none; color: #666; font-size: 24px; cursor: pointer;\">\n    <i class=\"fas fa-bars\"></i>\n  </button>\n</div>\n\n<!-- Mobile Menu (Hidden by default) -->\n<div id=\"clipup-mobile-menu\" class=\"clipup-mobile-menu\" style=\"display: none; position: fixed; top: 64px; left: 0; right: 0; background-color: white; border-top: 1px solid #eee; border-bottom: 1px solid #eee; box-shadow: 0 4px 10px rgba(0,0,0,0.1); z-index: 1000;\">\n  <div style=\"padding: 20px;\">\n    <div style=\"display: flex; flex-direction: column; gap: 16px;\">\n      <a href=\"#como-funciona\" style=\"color: #666; font-size: 16px; text-decoration: none; padding: 8px 0; transition: color 0.3s ease;\">Como Funciona</a>\n      <a href=\"#servicos\" style=\"color: #666; font-size: 16px; text-decoration: none; padding: 8px 0; transition: color 0.3s ease;\">Serviços</a>\n      <a href=\"#earnings\" style=\"color: #666; font-size: 16px; text-decoration: none; padding: 8px 0; transition: color 0.3s ease;\">Ganhos</a>\n      <a href=\"#perguntas\" style=\"color: #666; font-size: 16px; text-decoration: none; padding: 8px 0; transition: color 0.3s ease;\">FAQ</a>\n    </div>\n    \n    <div style=\"display: flex; flex-direction: column; gap: 12px; margin-top: 20px;\">\n      <a href=\"#\" style=\"display: block; text-align: center; background-color: var(--e-global-color-primary); color: white; font-size: 16px; font-weight: 500; padding: 12px; border-radius: 6px; text-decoration: none; transition: background-color 0.3s ease;\">Entrar</a>\n      \n      <a href=\"#\" style=\"display: block; text-align: center; border: 1px solid #ddd; background-color: transparent; color: #333; font-size: 16px; font-weight: 500; padding: 12px; border-radius: 6px; text-decoration: none; transition: all 0.3s ease;\">Criar Conta</a>\n    </div>\n  </div>\n</div>\n\n<script>\n  document.addEventListener('DOMContentLoaded', function() {\n    // Handle mobile menu toggle\n    const mobileToggle = document.getElementById('clipup-mobile-toggle');\n    const mobileMenu = document.getElementById('clipup-mobile-menu');\n    \n    if (mobileToggle && mobileMenu) {\n      mobileToggle.addEventListener('click', function() {\n        if (mobileMenu.style.display === 'none' || mobileMenu.style.display === '') {\n          mobileMenu.style.display = 'block';\n          mobileToggle.innerHTML = '<i class=\"fas fa-times\"></i>';\n        } else {\n          mobileMenu.style.display = 'none';\n          mobileToggle.innerHTML = '<i class=\"fas fa-bars\"></i>';\n        }\n      });\n    }\n    \n    // Handle responsive display\n    function handleResize() {\n      const desktopMenu = document.querySelector('.clipup-desktop-menu');\n      const mobileToggle = document.querySelector('.clipup-mobile-menu-toggle');\n      \n      if (window.innerWidth < 992) {\n        if (desktopMenu) desktopMenu.style.display = 'none';\n        if (mobileToggle) mobileToggle.style.display = 'flex';\n      } else {\n        if (desktopMenu) desktopMenu.style.display = 'flex';\n        if (mobileToggle) mobileToggle.style.display = 'none';\n        if (mobileMenu) mobileMenu.style.display = 'none';\n      }\n    }\n    \n    // Initial check\n    handleResize();\n    \n    // Listen for window resize\n    window.addEventListener('resize', handleResize);\n  });\n</script>",
