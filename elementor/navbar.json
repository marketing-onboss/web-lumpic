                                        {
                                            "id": "nav-logo123",
                                            "elType": "widget",
                                            "settings": {
                                                "editor": "<div style=\"display: flex; align-items: center; height: 64px;\">\n  <a href=\"#\" style=\"text-decoration: none;\">\n    <span style=\"font-size: 24px; font-weight: 700;\">\n      <span style=\"color: var(--e-global-color-primary);\">Clip</span><span style=\"color: #333;\">Up</span>\n    </span>\n  </a>\n</div>",
                                                "_margin": {
